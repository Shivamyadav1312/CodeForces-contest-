//Nene Game//
#include <bits/stdc++.h>
using namespace std;
void solve (){
    int k,q;
    cin>>k>>q;
    vector<int>v(k),queries(q);
    for(auto &i:v)
    cin>>i;
    for(auto &i:queries){
        cin>>i;
        if (i>=v[0]){
            cout<<v[0]-1<<" ";
            
        }else{
            cout<<i<<" ";
        }
    }
    cout<<"\n";
}
int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
//Nene and Card Game//
#include <bits/stdc++.h>
using namespace std;
void solve (){
    int n;
    cin>>n;
    vector<int>v(n);
    int ans=0;
    map<int,int>mp;
    for(auto &i:v){
        cin>>i;
        mp[i]++;
        if(mp[i]==2){
            ans++;
            
        }
    }
    cout<<ans<<"\n";
}
int main(){
    int t;
    cin>>t;
    while (t--){
        solve();
    }
    return 0;
}



