//Nene Game//
#include <bits/stdc++.h>
using namespace std;
void solve (){
    int k,q;
    cin>>k>>q;
    vector<int>v(k),queries(q);
    for(auto &i:v)
    cin>>i;
    for(auto &i:queries){
        cin>>i;
        if (i>=v[0]){
            cout<<v[0]-1<<" ";
            
        }else{
            cout<<i<<" ";
        }
    }
    cout<<"\n";
}
int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
    return 0;
}
